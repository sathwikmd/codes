int singleNumber(int* nums, int numsSize){
    // Initialize the unique number...
    int uniqNum = 0;
    // TRaverse all elements through the loop...
    for (int idx = 0; idx < numsSize; idx++) {
        // Concept of XOR...
        uniqNum = uniqNum ^ nums[idx];
    } return uniqNum;       // Return the unique number...
}
